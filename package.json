{
	"name": "terminally-swell",
	"displayName": "Terminally Swell",
	"description": "This extension will open a terminal to the path of the currently open file.",
	"publisher": "josiahpowell",
	"repository": {"url": "https://github.com/JoePall/terminally-swell"},
	"author": {
		"email": "josiahpowell@outlook.com",
		"name": "Josiah Powell",
		"url": "https://github.com/JoePall/"
	},
	"icon": "assets/swell.png",
	"version": "0.1.1",
	"engines": {
		"vscode": "^1.47.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:terminally-swell.terminallySwell"
	],
	"main": "./extension.js",
	"contributes": {
		"commands": [
			{
				"command": "terminally-swell.terminallySwell",
				"title": "Terminally Swell"
			}
		],
		"keybindings": [
			{
				"command": "terminally-swell.terminallySwell",
				"key": "ctrl+alt+n",
				"mac": "cmd+alt+n",
				"when": "editorTextFocus"
			}
		]
	},
	"scripts": {
		"lint": "eslint .",
		"pretest": "npm run lint",
		"test": "node ./test/runTest.js"
	},
	"devDependencies": {
		"@types/vscode": "^1.47.0",
		"@types/glob": "^7.1.3",
		"@types/mocha": "^8.0.0",
		"@types/node": "^14.0.27",
		"eslint": "^7.6.0",
		"glob": "^7.1.6",
		"mocha": "^8.0.1",
		"typescript": "^3.8.3",
		"vscode-test": "^1.4.0"
	}
}