{
	"name": "terminally-swell",
	"displayName": "Terminally Swell",
	"description": "Runs node on current file or Opens Terminal Here.",
	"publisher": "josiahpowell",
	"repository": {
		"url": "https://github.com/JoePall/terminally-swell"
	},
	"author": {
		"email": "josiahpowell@outlook.com",
		"name": "Josiah Powell",
		"url": "https://github.com/JoePall/"
	},
	"icon": "assets/swell.png",
	"version": "0.15.0",
	"engines": {
		"vscode": "^1.47.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:terminally-swell.runInNode",
		"onCommand:terminally-swell.runInNodeHard",
		"onCommand:terminally-swell.npmTest",
		"onCommand:terminally-swell.npmTestHard",
		"onCommand:terminally-swell.terminalHere",
		"onCommand:terminally-swell.terminalHereHard"
	],
	"main": "./extension.js",
	"contributes": {
		"commands": [
			{
				"command": "terminally-swell.runInNode",
				"title": "Run using Node"
			},
			{
				"command": "terminally-swell.runInNodeHard",
				"title": "Run using Node (Restart)"
			},
			{
				"command": "terminally-swell.herokuPush",
				"title": "Push git master to Heroku"
			},
			{
				"command": "terminally-swell.herokuPushHard",
				"title": "Push git master to Heroku (Restart)"
			},
			{
				"command": "terminally-swell.npmTest",
				"title": "Test"
			},
			{
				"command": "terminally-swell.npmTestHard",
				"title": "Test (Restart)"
			},
			{
				"command": "terminally-swell.terminalHere",
				"title": "Open Terminal Here"
			},
			{
				"command": "terminally-swell.terminalHereHard",
				"title": "Open Terminal Here (Restart)"
			},
			{
				"command": "terminally-swell.discardAllTerminals",
				"title": "Restart Terminals"
			}
		],
		"keybindings": [
			{
				"command": "terminally-swell.runInNode",
				"key": "alt+n",
				"mac": "alt+n"
			},
			{
				"command": "terminally-swell.runInNodeHard",
				"key": "alt+shift+n",
				"mac": "alt+shift+n"
			},
			{
				"command": "terminally-swell.npmTest",
				"key": "alt+t",
				"mac": "alt+t"
			},
			{
				"command": "terminally-swell.npmTestHard",
				"key": "alt+shift+t",
				"mac": "alt+shift+t"
			},
			{
				"command": "terminally-swell.herokuPush",
				"key": "alt+u",
				"mac": "alt+u"
			},
			{
				"command": "terminally-swell.herokuPushHard",
				"key": "alt+shift+u",
				"mac": "alt+shift+u"
			},
			{
				"command": "terminally-swell.terminalHere",
				"key": "alt+p",
				"mac": "alt+p"
			},
			{
				"command": "terminally-swell.terminalHereHard",
				"key": "alt+shift+p",
				"mac": "alt+shift+p"
			},
			{
				"command": "terminally-swell.discardAllTerminals",
				"key": "ctrl+shift+w",
				"mac": "ctrl+shift+w"
			}
		]
	},
	"scripts": {
		"lint": "eslint .",
		"pretest": "npm run lint",
		"test": "node ./test/runTest.js"
	},
	"devDependencies": {
		"@types/vscode": "^1.47.0",
		"@types/glob": "^7.1.3",
		"@types/mocha": "^8.0.0",
		"@types/node": "^14.0.27",
		"eslint": "^7.6.0",
		"glob": "^7.1.6",
		"mocha": "^8.0.1",
		"typescript": "^3.8.3",
		"vscode-test": "^1.4.0"
	}
}
